{
  "manifest_version": 3,
  "name": "LiveCodes Test Extension",
  "version": "0.0.1",
  "description": "Test context menu for LiveCodes",
  "permissions": ["contextMenus", "scripting", "activeTab", "tabs"],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [{
    "matches":["<all_urls>"],
    "js":["scripts/highlight.min.js","scripts/scrtipt.js"],
    "run_at": "document_idle",
  "all_frames": true
  }]
}
